{
   "title": "Your Details",
   "type": "object",
   "required": [
      "name",
      "last"
   ],
   "properties": {
      "avatarFile": {
         "type": "string",
         "format": "data-url"
      },
      "name": {
         "type": "string",
         "minLength": 3,
         "default": ""
      },
      "last": {
         "type": "string",
         "default": ""
      },
      "title": {
         "title": "",
         "type": "string",
         "enum": [
            "title1",
            "title2",
            "title3"
         ]
      },
      "lob": {
         "title": "",
         "type": "string",
         "enum": [
            "Entertainment",
            "LOB2",
            "LOB3"
         ]
      },
      "domain": {
         "type": "string",
         "enum": [
            "Advertisement",
            "Domain2",
            "Domain3"
         ]
      },
      "phone": {
         "type": "string",
         "title": "phone"
      },
      "email": {
         "type": "string",
         "title": "Select Domain"
      }
   }
}